{
  "name": "express-api-utils",
  "version": "1.0.2",
  "description": "A collection of utility functions for Express.js applications",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "exports": {
    ".": {
      "require": "./dist/cjs/index.js",
      "import": "./dist/esm/index.js"
    }
  },
  "type": "module",
  "files": [
    "dist/",
    "LICENSE",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "xcopy /E /I /Y src dist\\esm",
    "build:cjs": "babel src --out-dir dist/cjs --plugins @babel/plugin-transform-modules-commonjs",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aditya-Attrish/express-api-utils.git"
  },
  "keywords": [
    "express",
    "express-api-utils",
    "utils",
    "error-handling",
    "async-handler",
    "async",
    "await",
    "api",
    "api-format",
    "response-format"
  ],
  "author": "Aditya Attrish <aditayaattrish@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Aditya-Attrish/express-api-utils/issues"
  },
  "homepage": "https://github.com/Aditya-Attrish/express-api-utils#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@babel/cli": "^7.23.0",
    "@babel/core": "^7.23.0",
    "@babel/plugin-transform-modules-commonjs": "^7.23.0"
  }
}
